          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Download CSV" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">//csvurl: property of the url where the csvfiles are available
csvurl=props.get(&quot;csvurl&quot;).toString()
//csvfilename: property of the file (or list of files separated by ; to be downloaded)
csvfile=props.get(&quot;csvfilename&quot;).toString()

//url: the url of the file to be downloaded
//filename: the name of the file (with the extention) on the destination
def redirectFollowingDownload( String url, String filename ) {
  while( url ) {
    new URL( url ).openConnection().with { conn -&gt;
      conn.instanceFollowRedirects = false
      url = conn.getHeaderField( &quot;Location&quot; )      
      if( !url ) {
        new File( filename ).withOutputStream { out -&gt;
          conn.inputStream.with { inp -&gt;
            out &lt;&lt; inp
            inp.close()
          }
        }
      }
    }
  }
}
log.warn(&quot;**********************&quot;)
log.warn(&quot;Starting download&quot;)

//log.warn(csvurl)
//log.warn(csvfile)

csvfiles=csvfile.tokenize(&quot;;&quot;)
csvfiles.each{ file -&gt;
	redirectFollowingDownload(csvurl.toString()+&quot;/&quot;+file.toString(),file.toString())
	log.warn(&quot;downloaded &quot;+file.toString())
}


log.warn(&quot;Download done&quot;)
log.warn(&quot;**********************&quot;)</stringProp>
          </JSR223Sampler>
